    "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
  },
  "keywords": ["pesawise", "payments"],
  "author": "Richard",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
